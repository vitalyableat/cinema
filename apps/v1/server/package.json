{
  "name": "cinema-v1-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "../../../v1/server/node_modules/.bin/nest build",
    "format": "../../../v1/server/node_modules/.bin/prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "../../../v1/server/node_modules/.bin/nest start",
    "start:dev": "../../../v1/server/node_modules/.bin/nest start --watch",
    "start:debug": "../../../v1/server/node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "../../../v1/server/node_modules/.bin/eslint \\\"{src,apps,libs,test}/**/*.ts\\\" --fix",
    "test": "../../../v1/server/node_modules/.bin/jest",
    "test:watch": "../../../v1/server/node_modules/.bin/jest --watch",
    "test:cov": "../../../v1/server/node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../../../v1/server/node_modules/.bin/jest --runInBand",
    "test:e2e": "../../../v1/server/node_modules/.bin/jest --config ./test/jest-e2e.json",
    "typeorm": "../../../v1/server/node_modules/.bin/ts-node node_modules/typeorm/cli",
    "typeorm:create-migration": "npm run typeorm -- migration:create migrations/%npm_config_name%",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d typeorm.config.ts",
    "typeorm:revert-migrations": "npm run typeorm migration:revert -- -d typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "dotenv": "^16.4.5",
    "minio": "^8.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
